algoritmo "Área de um retangulo"

var
       area_retangulo,base_retangulo,altura_retangulo:real
inicio
      escreva("Informe o valor da base: ")
      leia(base_retangulo)
      escreva("Informe o valor da altura: ")
      leia(altura_retangulo)
      area_retangulo <- altura_retangulo * base_retangulo
      escreva("A área do retangulo é: ")
      escreva(area_retangulo)
fimalgoritmo
