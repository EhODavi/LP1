algoritmo "Troca de valores"

var
   numero1,numero2:real
inicio
      escreva("Informe o primeiro numero: ")
      leia(numero1)
      escreva("Informe o segundo numero: ")
      leia(numero2)
      escreval("O valor do primeiro numero é: ")
      escreval(numero2)
      escreval("O valor do segundo numero é: ")
      escreval(numero1)
fimalgoritmo
