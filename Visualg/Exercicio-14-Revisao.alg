algoritmo "Exercício 14-Revisão"

var
soma,contador,quantidade:inteiro
media:real
inicio
media <- 0
soma <- 0
quantidade <- 0
para contador de 2 ate 100 passo 2 faca
   escreval(contador)
   soma <- soma + contador
   quantidade <- quantidade + 1
fimpara
media <- soma/quantidade
escreval("A média dos numeros é: ")
escreval(media)
fimalgoritmo
