algoritmo "Consumo médio"

var
    consuMedio,distanciatotal,totalcombustivel:real
inicio
      escreva("Qual foi a distancia total percorrida: ")
      leia(distanciatotal)
      escreva("Qual foi o total do combustivel gasto: ")
      leia(totalcombustivel)
      consuMedio <- distanciatotal/totalcombustivel
      escreva("O consumo médio é: ")
      escreva(consuMedio)
fimalgoritmo
